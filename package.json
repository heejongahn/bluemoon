{
  "name": "bluemoon",
  "version": "1.0.0",
  "main": "index.js",
  "repository": {
    "url": "https://github.com/heejongahn/bluemoon",
    "type": "git"
  },
  "author": "Heejong Ahn <heejongahn@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "normalize.css": "^5.0.0",
    "open-color": "^1.4.0",
    "stylus": "^0.54.5"
  },
  "scripts": {
    "build-stylus": "stylus src/bluemoon.styl -o dist/bluemoon.css",
    "build-js": "babel src/bluemoon.js --out-file dist/bluemoon.js",
    "build": "npm run build-stylus && npm run build-js",
    "watch": "stylus src/bluemoon.styl -o dist -w",
    "prepublish": "npm run build"
  }
}
